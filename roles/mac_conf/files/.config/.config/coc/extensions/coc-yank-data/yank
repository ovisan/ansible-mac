	  Dependency infra
	end title
	
	storage BDRIT <<environment>> as "BDR IT"{
	  folder Isilon as "BDR IT Isilon S3"{
	    storage s3prd <<bucket>> as "s3-fdz-prd-public"
	  }
	}
	
	storage TIR <<environment>> as "TIR" {
	  entity tirfirewall as "pfsense Firewall"
	  entity tirproxy as "TIR Proxy"
	  entity keycloak as "Keycloak"
	}
	
	storage vSphere <<environment>> {
	  ' NFS
	  folder "sap-hana-nfs-backup" {
	    DEV2_REDHAT(hananfsbackup,fdz-nfs-02)
	  }
	
	  ' SAP Hana
	  folder "devqas-integrated-storage" {
	    DEV2_REDHAT(devqasHanaStorage,fdz-nfs-devqas-int-01)
	  }
	  folder "devqas-integrated-hana" {
	    DEV2_REDHAT(devqasHanaCockpit,fdz-hana-cockpit-devqas-int01)
	    DEV2_REDHAT(devqasHanaDB,fdz-hanadb-devqas-int01)
	    devqasHanaDB -- devqasHanaCockpit
	    devqasHanaDB --> devqasHanaStorage
	  }
	
	  folder "prd-integrated-storage" {
	    DEV2_REDHAT(prdHanaStorage,fdz-nfs-prd-int-01)
	  }
	  folder "prd-integrated-hana" {
	    DEV2_REDHAT(prdHanaCockpit,fdz-hana-cockpit-prd-int01)
	    DEV2_REDHAT(prdHanaDB,fdz-hanadb-prd-int01)
	    prdHanaDB -- prdHanaCockpit
	    prdHanaDB --> prdHanaStorage
	  }
	
	  ' Rancher clusters
	  DEV2_KUBERNETES_WORDMARK(local,local,node,blue) #LightGreen{
	    collections rancher
	    note top of rancher: https://rancher.mgmt.fdz.tir.bdr.de
	  }
	  DEV2_KUBERNETES_WORDMARK(cluster1,ds-devqas-integrated,node,blue) #LightGreen
	  DEV2_KUBERNETES_WORDMARK(cluster2,ds-devqas-isolated,node,blue) #LightGreen
	  DEV2_KUBERNETES_WORDMARK(cluster3,infra-devqas-integrated,node,blue) #LightGreen
	  DEV2_KUBERNETES_WORDMARK(cluster4,infra-devqas-isolated,node,blue) #LightGreen
	  DEV2_KUBERNETES_WORDMARK(cluster5,infra-mgmt,node,blue) #LightGreen
	  DEV2_KUBERNETES_WORDMARK(cluster6,infra-prd-integrated,node,blue) #LightGreen
	  DEV2_KUBERNETES_WORDMARK(cluster7,sapdi-devqas-isolated,node,blue) #LightGreen
	  DEV2_KUBERNETES_WORDMARK(cluster8,sapdi-prd-isolated,node,blue) #LightGreen
	
	  rancher -- cluster1
	  rancher -- cluster2
	  rancher -- cluster3
	  rancher -- cluster4
	  rancher -- cluster5
	  rancher -- cluster6
	  rancher -- cluster7
	  rancher -- cluster8
	
	  ' Helm charts
	  together {
	    package vault<<helm chart>>
	    note top of vault: https://vault.infra.mgmt.fdz.tir.bdr.de
	    vault -l-> s3prd : "backup"
	    vault -u-> cluster5
	    package minio<<helm chart>>
	    minio -u-> cluster5
	    package postgresql<<helm chart>>
	    postgresql -u-> cluster5
	  }
	
	  ' Individual rectangles
	  DEV2_REDHAT(ipa,IPA DNS Server)
	  note top of ipa: https://ipa.mgmt.fdz.tir.bdr.de
	
	  DEV2_REDHAT(firewall,pfsense Firewall)
	
	  FA5_GITLAB(gitlab1,Gitlab runner,rectangle)
	
	  DEV2_NGINX_ORIGINAL(reverseproxy,Reverse Proxy)
	
	  ' VDI (Windows)
	  together {
	    folder "vdi-devqas" {
	      folder "vdi-dev"{
	        DEV_WINDOWS(vdi1,fdz-tsdev01) #DeepSkyBlue
	        DEV_WINDOWS(vdi2,fdz-tsdev02) #DeepSkyBlue
	      }
	      folder "vdi-qas" {
	        DEV_WINDOWS(vdi3,fdz-tsqas01) #DeepSkyBlue
	        DEV_WINDOWS(vdi4,fdz-tsqas02) #DeepSkyBlue
	      }
	    }
	    folder "vdi-domain" {
	      DEV_WINDOWS(vdi5,fdz-subca01) #DeepSkyBlue
	      DEV_WINDOWS(vdi6,fdz-dc1) #DeepSkyBlue
	      DEV_WINDOWS(vdi7,fdz-dc2) #DeepSkyBlue
	      DEV_WINDOWS(vdi8,fdz-rdcb) #DeepSkyBlue
	    }
	    folder "vdi-infra" {
	      DEV_WINDOWS(vdi9,fdz-ctxcon01) #DeepSkyBlue
	      DEV_WINDOWS(vdi10,fdz-ctxcon02) #DeepSkyBlue
	      DEV_WINDOWS(vdi11,fdz-file01) #DeepSkyBlue
	      DEV_WINDOWS(vdi12,fdz-rdgw) #DeepSkyBlue
	      DEV_WINDOWS(vdi13,fdz-rdweb) #DeepSkyBlue
	    }
	    folder "vdi-prd" {
	      DEV_WINDOWS(vdi14,fdz-tsprd01) #DeepSkyBlue
	      DEV_WINDOWS(vdi15,fdz-tsprd02) #DeepSkyBlue
	    }
	
	    entity sapdiprdiso as "SAP DI"
	    sapdiprdiso -u-> cluster8
	    entity sapdidevqasiso as "SAP DI"
	    sapdidevqasiso -u-> cluster7
