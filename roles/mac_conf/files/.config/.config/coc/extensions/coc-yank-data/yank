61599d5c33d6b2590e85739fa8af4702|file:///Users/ovidiuvisan/code/diagrams/infrastructure.md|2|1|V|markdown
	<style>
	collections {
	  LineThickness 2
	  LineColor Black
	  BackgroundColor RoyalBlue
	  FontSize 18
	}
	</style>
	
	' Distancing between elements
	skinparam nodesep 20
	skinparam ranksep 350
	
	' Beautfy
	skinparam title {
	  backgroundColor APPLICATION
	}
	
	skinparam component<<vm>> {
	  backgroundColor LightCoral
	}
	
	skinparam node<<k8s cluster>> {
	  backgroundColor LightGreen
	}
	
	skinparam folder {
	  backgroundColor Ivory
	}
	
	skinparam package<<helm chart>> {
	  backgroundColor LightCyan
	}
	
	skinparam storage<<bucket>> {
	  backgroundColor LightSteelBlue
	}
	
	skinparam storage<<environment>> {
	  backgroundColor Snow
	}
	
	title
	  Dependency infra
	end title
	
	storage BDRIT <<environment>> as "BDR IT"{
	  folder Isilon as "BDR IT Isilon S3"{
	    storage s3prd <<bucket>> as "s3-fdz-prd-public"
	  }
	}
	
	storage TIR <<environment>> as "TIR" {
	  component tirfirewall <<vm>> as "pfsense Firewall"
	  component tirproxy <<vm>> as "TIR Proxy"
	}
	
	storage vSphere <<environment>> {
	  ' NFS
	  folder "sap-hana-nfs-backup" {
	    component hananfsbackup as "fdz-nfs-02" <<vm>>
	  }
	
	  ' SAP Hana
	  folder "devqas-integrated-storage" {
	    component devqasHanaStorage as "fdz-nfs-devqas-int-01" <<vm>>
	  }
	  folder "devqas-integrated-hana" {
	    component devqasHanaCockpit as "fdz-hana-cockpit-devqas-int01" <<vm>>
	    component devqasHanaDB as "fdz-hanadb-devqas-int01" <<vm>>
	    devqasHanaDB -- devqasHanaCockpit
	    devqasHanaDB --> devqasHanaStorage
	  }
	
	  folder "prd-integrated-storage" {
	    component prdHanaStorage as "fdz-nfs-prd-int-01" <<vm>>
	  }
	  folder "prd-integrated-hana" {
	    component prdHanaCockpit as "fdz-hana-cockpit-prd-int01" <<vm>>
	    component prdHanaDB as "fdz-hanadb-prd-int01" <<vm>>
	    prdHanaDB -- prdHanaCockpit
	    prdHanaDB --> prdHanaStorage
	  }
	
	  ' Rancher clusters
	  node local <<k8s cluster>> as "Local cluster"{
	    collections rancher
	    note top of rancher: https://rancher.mgmt.fdz.tir.bdr.de
	  }
	  node cluster1 <<k8s cluster>> as "ds-devqas-integrated"
	  node cluster2 <<k8s cluster>> as "ds-devqas-isolated"
	  node cluster3 <<k8s cluster>> as "infra-devqas-integrated"
	  node cluster4 <<k8s cluster>> as "infra-devqas-isolated"
	  node cluster5 <<k8s cluster>> as "infra-mgmt"
	  node cluster6 <<k8s cluster>> as "infra-prd-integrated"
	  node cluster7 <<k8s cluster>> as "sapdi-devqas-isolated"
	  node cluster8 <<k8s cluster>> as "sapdi-prd-isolated"
	
	  rancher -- cluster1
	  rancher -- cluster2
	  rancher -- cluster3
	  rancher -- cluster4
	  rancher -- cluster5
	  rancher -- cluster6
	  rancher -- cluster7
	  rancher -- cluster8
	
	  ' Helm charts
	  together {
	    package vault<<helm chart>>
	    note top of vault: https://vault.infra.mgmt.fdz.tir.bdr.de
	    vault --> s3prd : "backup"
	    vault --> cluster5
	    package minio<<helm chart>>
	    minio --> cluster5
	    package postgresql<<helm chart>>
	    postgresql --> cluster5
	  }
	
	  ' Individual components
	  component ipa <<vm>> as "IPA DNS Server"
	  note top of ipa: https://ipa.mgmt.fdz.tir.bdr.de
	
	  component firewall <<vm>> as "pfsense Firewall"
	
	  component gitlab <<vm>> as "Gitlab runner"
	
	  component reverseproxy <<vm>> as "Reverse Proxy"
	
	  ' VDI (Windows)
	  together {
	    folder "vdi-devqas" {
	      folder "vdi-dev"{
	        component vdi1 <<vm>> as "fdz-tsdev01"
	        component vdi2 <<vm>> as "fdz-tsdev02"
	      }
	      folder "vdi-qas" {
	        component vdi3 <<vm>> as "fdz-tsqas01"
	        component vdi4 <<vm>> as "fdz-tsqas02"
	      }
	    }
	    folder "vdi-domain" {
	      component vdi5 <<vm>> as "fdz-subca01"
	    }
	    folder "vdi-infra" {
	      component vdi5 <<vm>> as "fdz-ctxcon01"
	      component vdi6 <<vm>> as "fdz-ctxcon02"
	      component vdi7 <<vm>> as "fdz-file01"
	    }
	    folder "vdi-prd" {
	      component vdi8 <<vm>> as "fdz-tsprd01"
	      component vdi9 <<vm>> as "fdz-tsprd02"
	    }
	
	    entity sapdiprdiso as "SAP DI"
	    sapdiprdiso --> cluster8
	    entity sapdidevqasiso as "SAP DI"
	    sapdidevqasiso --> cluster7
	  }
	}
