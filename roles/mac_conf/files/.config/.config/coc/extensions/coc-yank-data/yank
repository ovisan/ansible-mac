	@startuml
	<style>
	collections {
	  LineThickness 2
	  LineColor Black
	  BackgroundColor RoyalBlue
	  FontSize 18
	}
	</style>
	
	!include <cloudogu/tools/k8s.puml>
	
	' Distancing
	' horizontal
	skinparam nodesep 20
	' vertical
	skinparam ranksep 10
	' distance between elements
	skinparam padding 10
	
	' Beautfy
	skinparam title {
	  backgroundColor APPLICATION
	}
	
	skinparam rectangle<<vm>> {
	  backgroundColor LightCoral
	}
	
	skinparam node<<k8s cluster>> {
	  backgroundColor LightGreen
	}
	
	skinparam folder {
	  backgroundColor Ivory
	}
	
	skinparam package<<helm chart>> {
	  backgroundColor LightCyan
	}
	
	skinparam storage<<bucket>> {
	  backgroundColor LightSteelBlue
	}
	
	skinparam storage<<environment>> {
	  backgroundColor Snow
	}
	
	title
	  Dependency infra
	end title
	
	storage BDRIT <<environment>> as "BDR IT"{
	  folder Isilon as "BDR IT Isilon S3"{
	    storage s3prd <<bucket>> as "s3-fdz-prd-public"
	  }
	}
	
	storage TIR <<environment>> as "TIR" {
	  entity tirfirewall as "pfsense Firewall"
	  entity tirproxy as "TIR Proxy"
	}
	
	storage vSphere <<environment>> {
	  ' NFS
	  folder "sap-hana-nfs-backup" {
	    rectangle hananfsbackup as "fdz-nfs-02" <<vm>>
	  }
	
	  ' SAP Hana
	  folder "devqas-integrated-storage" {
	    rectangle devqasHanaStorage as "fdz-nfs-devqas-int-01" <<vm>>
	  }
	  folder "devqas-integrated-hana" {
	    rectangle devqasHanaCockpit as "fdz-hana-cockpit-devqas-int01" <<vm>>
	    rectangle devqasHanaDB as "fdz-hanadb-devqas-int01" <<vm>>
	    devqasHanaDB -- devqasHanaCockpit
	    devqasHanaDB --> devqasHanaStorage
	  }
	
	  folder "prd-integrated-storage" {
	    rectangle prdHanaStorage as "fdz-nfs-prd-int-01" <<vm>>
	  }
	  folder "prd-integrated-hana" {
	    rectangle prdHanaCockpit as "fdz-hana-cockpit-prd-int01" <<vm>>
	    rectangle prdHanaDB as "fdz-hanadb-prd-int01" <<vm>>
	    prdHanaDB -- prdHanaCockpit
	    prdHanaDB --> prdHanaStorage
	  }
	
	  ' Rancher clusters
	  TOOL_K8S(k8s, Test) #ffffff
	  node local <<k8s cluster>> as "Local cluster"{
	    collections rancher
	    note top of rancher: https://rancher.mgmt.fdz.tir.bdr.de
	  }
	  node cluster1 <<k8s cluster>> as "ds-devqas-integrated"
	  node cluster2 <<k8s cluster>> as "ds-devqas-isolated"
	  node cluster3 <<k8s cluster>> as "infra-devqas-integrated"
	  node cluster4 <<k8s cluster>> as "infra-devqas-isolated"
	  node cluster5 <<k8s cluster>> as "infra-mgmt"
	  node cluster6 <<k8s cluster>> as "infra-prd-integrated"
	  node cluster7 <<k8s cluster>> as "sapdi-devqas-isolated"
	  node cluster8 <<k8s cluster>> as "sapdi-prd-isolated"
	
	  rancher -- cluster1
	  rancher -- cluster2
	  rancher -- cluster3
	  rancher -- cluster4
	  rancher -- cluster5
	  rancher -- cluster6
	  rancher -- cluster7
	  rancher -- cluster8
	
	  ' Helm charts
	  together {
	    package vault<<helm chart>>
	    note top of vault: https://vault.infra.mgmt.fdz.tir.bdr.de
	    vault -l-> s3prd : "backup"
	    vault -u-> cluster5
	    package minio<<helm chart>>
	    minio -u-> cluster5
	    package postgresql<<helm chart>>
	    postgresql -u-> cluster5
	  }
	
	  ' Individual rectangles
	  rectangle ipa <<vm>> as "IPA DNS Server"
	  note top of ipa: https://ipa.mgmt.fdz.tir.bdr.de
	
	  rectangle firewall <<vm>> as "pfsense Firewall"
	
	  rectangle gitlab <<vm>> as "Gitlab runner"
	
	  rectangle reverseproxy <<vm>> as "Reverse Proxy"
	
	  ' VDI (Windows)
	  together {
	    folder "vdi-devqas" {
	      folder "vdi-dev"{
	        rectangle vdi1 <<vm>> as "fdz-tsdev01"
	        rectangle vdi2 <<vm>> as "fdz-tsdev02"
	      }
	      folder "vdi-qas" {
	        rectangle vdi3 <<vm>> as "fdz-tsqas01"
	        rectangle vdi4 <<vm>> as "fdz-tsqas02"
	      }
	    }
	    folder "vdi-domain" {
	      rectangle vdi5 <<vm>> as "fdz-subca01"
	      rectangle vdi6 <<vm>> as "fdz-dc1"
	      rectangle vdi7 <<vm>> as "fdz-dc2"
	      rectangle vdi8 <<vm>> as "fdz-rdcb"
	    }
	    folder "vdi-infra" {
	      rectangle vdi9 <<vm>> as "fdz-ctxcon01"
	      rectangle vdi10 <<vm>> as "fdz-ctxcon02"
	      rectangle vdi11 <<vm>> as "fdz-file01"
	      rectangle vdi12 <<vm>> as "fdz-rdgw"
	      rectangle vdi13 <<vm>> as "fdz-rdweb"
	    }
	    folder "vdi-prd" {
	      rectangle vdi14 <<vm>> as "fdz-tsprd01"
	      rectangle vdi15 <<vm>> as "fdz-tsprd02"
	    }
	
	    entity sapdiprdiso as "SAP DI"
	    sapdiprdiso -u-> cluster8
	    entity sapdidevqasiso as "SAP DI"
	    sapdidevqasiso -u-> cluster7
	  }
	}
	@enduml
	
