c27759e1d3a623170dcb94196e7d09f5|file:///Users/ovidiuvisan/code/diagrams/infrastructure.puml|1|1|V|plantuml
	@startuml
	<style>
	collections {
	  LineThickness 2
	  LineColor Black
	  BackgroundColor RoyalBlue
	  FontSize 18
	}
	</style>
	
	' Distancing between elements
	skinparam nodesep 20
	skinparam ranksep 350
	
	' Beautfy
	skinparam title {
	  backgroundColor APPLICATION
	}
	
	skinparam component<<vm>> {
	  backgroundColor LightCoral
	}
	
	skinparam node<<k8s cluster>> {
	  backgroundColor LightGreen
	}
	
	skinparam folder {
	  backgroundColor Ivory
	}
	
	skinparam package<<helm chart>> {
	  backgroundColor LightCyan
	}
	
	skinparam storage<<bucket>> {
	  backgroundColor LightSteelBlue
	}
	
	skinparam storage<<environment>> {
	  backgroundColor Snow
	}
	
	title
	  Dependency infra
	end title
	
	storage BDRIT <<environment>> as "BDR IT"{
	  folder Isilon as "BDR IT Isilon S3"{
	    storage s3prd <<bucket>> as "s3-fdz-prd-public"
	  }
	}
	
	storage TIR <<environment>> as "TIR"
	
	storage vSphere <<environment>> {
	  ' NFS
	  folder "sap-hana-nfs-backup" {
	    component hananfsbackup as "NFS" <<vm>>
	  }
	
	  ' SAP Hana
	  folder "devqas-integrated-hana" {
	    component devqasHanaDB as "Hana DB" <<vm>>
	    component devqasHanaCockpit as "Hana Cockpit" <<vm>>
	    devqasHanaDB -- devqasHanaCockpit
	    devqasHanaDB --> hananfsbackup
	  }
	  folder "prd-integrated-hana" {
	    component prdHanaDB as "Hana DB" <<vm>>
	    component prdHanaCockpit as "Hana Cockpit" <<vm>>
	    prdHanaDB -- prdHanaCockpit
	    prdHanaDB --> hananfsbackup
	  }
	
	  ' Rancher clusters
	  node local <<k8s cluster>> as "Local cluster"{
	    collections rancher
	    note top of rancher: https://rancher.mgmt.fdz.tir.bdr.de
	  }
	  node cluster1 <<k8s cluster>> as "ds-devqas-integrated"
	  node cluster2 <<k8s cluster>> as "ds-devqas-isolated"
	  node cluster3 <<k8s cluster>> as "infra-devqas-integrated"
	  node cluster4 <<k8s cluster>> as "infra-devqas-isolated"
	  node cluster5 <<k8s cluster>> as "infra-mgmt"
	  node cluster6 <<k8s cluster>> as "infra-prd-integrated"
	  node cluster7 <<k8s cluster>> as "sapdi-devqas-isolated"
	  node cluster8 <<k8s cluster>> as "sapdi-prd-isolated"
	
	  rancher -- cluster1
	  rancher -- cluster2
	  rancher -- cluster3
	  rancher -- cluster4
	  rancher -- cluster5
	  rancher -- cluster6
	  rancher -- cluster7
	  rancher -- cluster8
	
	  ' Helm charts
	  together {
	    package vault<<helm chart>>
	    note top of vault: https://vault.infra.mgmt.fdz.tir.bdr.de
	    vault --> s3prd : "backup"
	    vault --> cluster5
	    package minio<<helm chart>>
	    minio --> cluster5
	    package postgresql<<helm chart>>
	    postgresql --> cluster5
	  }
	
	  ' Individual components
	  component ipa <<vm>> as "IPA DNS Server"
	  note top of ipa: https://ipa.mgmt.fdz.tir.bdr.de
	
	  component firewall <<vm>> as "pfsense Firewall"
	
	  component gitlab <<vm>> as "Gitlab server"
	  note top of gitlab: https://partner.bdr.de/gitlab
	
	  entity sapdiprdiso as "SAP DI"
	  sapdiprdiso --> cluster8
	  entity sapdidevqasiso as "SAP DI"
	  sapdidevqasiso --> cluster7
	}
	@enduml
	
