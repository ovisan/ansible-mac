cedaa3620703b28d5b70fcd22dfee5a7|file:///Users/ovidiuvisan/.zshrc|188|1|V|zsh
	# fd - cd to selected directory
	fdd() {
	  local dir
	  if [[ $@ ]]
	  then
	    dir=$(find $@ -d | fzf-tmux)
	  else
	    dir=$(find ~ -d | fzf-tmux)
	  fi
	  cd "$dir"
	}
